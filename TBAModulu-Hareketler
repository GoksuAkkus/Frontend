+  dotMethod(value: string) {
+    let splitDotArray = [];
+    let valueWithoutDot = '';
+    if (value.includes('.')) {
+      splitDotArray = value.split('.');
+      for (let i = 0; i < splitDotArray.length; i++) {
+        valueWithoutDot += splitDotArray[i];
+      }
+      value = valueWithoutDot;
+    }
+
+    const length = 3;
+
+    const splitComma = value.split(',');
+    const dotValue = splitComma[0];
+    const commaValue = splitComma[1];
+
+    const stringLength = dotValue.length;
+    const loopNumber = Math.ceil(stringLength / length);
+
+    const splittedValues = [];
+    let dottedString = '';
+    for (let i = 0; i < loopNumber; i++) {
+      const splittedValue = dotValue.substring(stringLength - (length * (i + 1)), stringLength - (length * i));
+      splittedValues.push(splittedValue);
+    }
+
+    const reversedValues = splittedValues.reverse();
+    reversedValues.forEach(stringValue => {
+      dottedString += stringValue + '.';
+    });
+
+    dottedString = dottedString.slice(0, -1);
+    if (commaValue !== undefined) {
+      dottedString += ',' + commaValue;
+    }
+
+    return dottedString;
+  }
+
