+  async getLocation() {
+
+    const pointProp: esri.PointProperties = {
+      x: Number(this.lat),
+      y: Number(this.lon),
+      spatialReference: { wkid: 4326 }
+    };
+
+    // TODO: POINTMODULE IS NOT A CONSTRUCTOR HATASI ÇÖZÜLECEK
+    const point = new this.pointModule(pointProp);
+
+    const properties: esri.QueryProperties = {
+      where: '1=1',
+      geometry: point,
+      spatialRelationship: 'Intersects',
+      outFields: ['*'],
+      returnGeometry: false
+    };
+    // BURADA HANGİ İLÇEDE OLDUĞUNU BULUP YETKİSİ OLUP OLMADIĞINA BAKILACAK
+    this.ilceLayer.queryFeatures(properties).then(res => {
+      console.log(res);
+    });
+  }
+
+  isPermitted() {
+    const uavtil = this.ilceLayer['attributes']['uavtil'];
+    const ilceKodu = this.ilceLayer['attributes']['ilce'];
+  }
+
+  setPermZones() {
+    const permZones = this.userPerm.permZone;
+
+    permZones.forEach(permZone => {
+      const zone = permZone.zone;
+      if (zone.includes(',')) { // , içerenler izin verilen 1 ilin ilçe listesi demek
+        this.permittedDistricts.push(zone.split(','));
+      } else {
+        this.permittedCities.push(zone);
+      }
+    });
   }
+
+  getAllNumaratajFromBina(lat: string, lon: string) {
+    this.webDataService.getAllNumaratajFromBina(lat, lon).subscribe(res => {
+      if (res !== null || res !== undefined) {
+        this.binaList = res;
+      } else {
+        this.maksBinaKoduSorgulaState = false;
+        this.notificationMessage = 'Maks:Yapi ve Maks:DigerYapida Veri Bulunamadı.';
+        // TODO: res NUll dönüyorsa maks:yapi ve maks:digeryapida veri bulamamış demek info dialog ile bulunamadı uyarısı versin
+      }
+    });
+  }
