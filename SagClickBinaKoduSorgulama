   ngOnInit() {
+    this.userPerm = this.auth.userPerm;
+    const data = this.dialogConfig.data;
+    this.mapView = data.mapView;
+    this.maksBinaKoduSorgulaState = data['maksBinaKoduSorgulaState'];
