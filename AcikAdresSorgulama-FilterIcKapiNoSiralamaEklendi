+      }
+    });
+
+    this.icKapiNoFilterArray = tempArray;
+    this.resetFormData(formElement, inputContainer);
+  }
+
+  getIndexElement(inputContainerElement: HTMLElement): number {
+    let previousElementSibling = inputContainerElement.previousElementSibling;
+    let index = 0;
+    while (previousElementSibling !== null) {
+      if (!previousElementSibling.classList.contains('select-options')) {
+        index++;
+      }
+      previousElementSibling = previousElementSibling.previousElementSibling;
+    }
+
+    return index;
+  }
