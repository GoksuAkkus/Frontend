+  resetFormData(form: HTMLFormElement, inputContainerElement: HTMLElement) {
+    const formElements: HTMLCollectionOf<HTMLFormElement> = <HTMLCollectionOf<HTMLFormElement>>form.elements;
+    const formElementsLength = formElements.length;
+    const index = this.getIndexElement(inputContainerElement);
+
+    for (let i = index + 1; i < formElementsLength; i++) {
+      const inputElement: HTMLFormElement = <HTMLFormElement>formElements[i];
+      if (inputElement !== undefined) {
+        inputElement.value = '';
+      }
+    }
+  }
+
+  resetGlobalVariables() {
+    this.acikAdres = '';
+    this.errorMessage = '';
+    this.binaArray = [];
+    this.icKapiNoFilterArray = [];
