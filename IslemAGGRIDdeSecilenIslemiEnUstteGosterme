+  colorYellowSelectedRow() {
+    const rows = document.querySelectorAll('[row-id="0"]');
+    let selectedRow: HTMLElement;
+    for (let i = 0; i < rows.length; i++) {
+      const clientWidth = rows[i].clientWidth;
+      const noFocusClass = rows[i].classList.contains('ag-row-no-focus');
+      if (clientWidth !== 0 && noFocusClass) {
+        selectedRow = rows[i] as HTMLElement;
+      }
+    }
+
+    selectedRow.style.backgroundColor = 'yellow';
+  }
