+    this.getMEBReports().subscribe(resServiceInfo => {
+      const groupLayerModel: Array<GroupLayerModel> = [];
+      resServiceInfo.layers.forEach(layer => {
+        if (layer.parentLayerId === -1) {
+          groupLayerModel.push(layer);
+        } else {
+          const parentLayerId = layer.parentLayerId;
+          const groupLayer = groupLayerModel[parentLayerId];
+
+          if (groupLayer !== undefined) {
+            if (groupLayer.subLayers === undefined) {
+              groupLayer.subLayers = [];
+            }
+
+            groupLayer.subLayers.push(layer);
+          } else {
+            console.error('Group Layer ID mevcut Değil');
+          }
+        }
+      });
+
+      this.mebGroupReports = groupLayerModel;
+    });
