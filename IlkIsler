diff --git a/src/app/report-table/report-table.component.ts b/src/app/report-table/report-table.component.ts
new file mode 100644
index 0000000000000000000000000000000000000000..1a4feeff94a39ad36eda9a945bd5d8eafebe1945
--- /dev/null
+++ b/src/app/report-table/report-table.component.ts
@@ -0,0 +1,470 @@
+import { Component, OnInit, ViewChild, OnDestroy } from '@angular/core';
+import { AgGridNg2 } from 'ag-grid-angular';
+import { DialogRef } from '../dialog/dialog-ref';
+import { DialogConfig } from '../dialog/dialog-config';
+import { DialogService } from '../dialog/dialog.service';
+import { HttpClient, HttpHeaders } from '@angular/common/http';
+import esri = __esri;
+import { UserPermission } from '../models/Login/user-permission';
+import { environment } from 'src/environments/environment';
+import { loadModules } from 'esri-loader';
+import { ColDef, IDatasource, IGetRowsParams, ColumnApi } from 'ag-grid-community';
+import { SortModel } from '../models/AGGrid/sort-model';
+import { FilterModel } from '../models/AGGrid/filter-model';
+import { QueryConstructor } from '../models/AGGrid/query-constructor';
+import { ProgressInfoComponent } from '../progress-info/progress-info.component';
+import { LayerSymbologyComponent } from '../layer-symbology/layer-symbology.component';
+
+@Component({
+  selector: 'app-report-table',
+  templateUrl: './report-table.component.html',
+  styleUrls: ['./report-table.component.scss']
+})
+export class ReportTableComponent implements OnInit, OnDestroy {
+  @ViewChild('agGrid', { static: true }) agGrid: AgGridNg2;
+
+  guiPermissions: Array<boolean> = [];
+  mapView: esri.MapView;
+  symbologyBuilderDialog: DialogRef;
+  whereQuery: string;
+
+  filterParams: esri.QueryProperties;
+  reportLayer: esri.FeatureLayer;
+
+  constructor(public dialogConfig: DialogConfig, public dialogRef: DialogRef, private dialog: DialogService, private http: HttpClient) { }
+
+  ngOnInit() {
+    const data = this.dialogConfig.data;
+
+    const mapView: esri.MapView = data.mapView;
+    const selectedLayerID = <number>data.selectedLayerID;
+    const userPermission = data.userPerm;
+
+    this.mapView = mapView;
+
+    this.fillGUIAuthorization(userPermission);
+    this.initalizeAGGrid();
+    loadModules(['esri/layers/FeatureLayer']).then(([FeatureLayer]) => {
+      const FeatureLayerConstructor = <esri.FeatureLayerConstructor>FeatureLayer;
+
+      const featureLayerProperties = <esri.FeatureLayerProperties>{
+        title: 'Deneme',
+        url: `${environment.arcgisMEBRaporURL}/${selectedLayerID}`,
+        visible: false,
+        minScale: 0,
+        maxScale: 0
+      };
+
+      const reportLayer = new FeatureLayerConstructor(featureLayerProperties);
+      mapView.map.add(reportLayer);
+
+      reportLayer.on('layerview-create', (event) => {
+        this.reportLayer = reportLayer;
+
+        reportLayer.fields.forEach(field => field.set('rAuthorized', true));
+        this.filterData();
+      });
+    });
+  }
+
+  fillGUIAuthorization(userPermission: UserPermission) {
+    this.guiPermissions.push(this.isAuthorized(userPermission, 0));
+    this.guiPermissions.push(this.isAuthorized(userPermission, 1));
+  }
+
+  isAuthorized(userPermission: UserPermission, id: number) {
+    if (userPermission === undefined) {
+      return true;
+    }
+
+    const flattenPermItems = userPermission.flattenPermGUI[1];
+
+    if (flattenPermItems !== undefined) {
+      if (flattenPermItems.items.includes(id)) {
+        return true;
+      } else {
+        return false;
+      }
+    } else {
+      return false;
+    }
+  }
+
+  initalizeAGGrid() {
+    this.agGrid.floatingFilter = true;
+    this.agGrid.pagination = true;
+    this.agGrid.paginationAutoPageSize = true;
+    this.agGrid.maxBlocksInCache = 2;
+    this.agGrid.infiniteInitialRowCount = 1;
+    this.agGrid.rowModelType = 'infinite';
+    this.agGrid.rowDeselection = true;
+    this.agGrid.rowSelection = 'single';
+    this.agGrid.defaultColDef = {
+      sortable: true,
+      resizable: true,
+      filter: true,
+      width: 100
+    };
+    this.agGrid.localeText = {
+      page: 'Sayfa',
+      more: 'Daha',
+      to: '->',
+      of: ' / ',
+      next: 'Sonraki',
+      previous: 'Önceki',
+      loadingOoo: 'Yükleniyor...',
+      equals: 'Eşit',
+      notEqual: 'Eşit Değil',
+      lessThan: 'Küçüktür',
+      greaterThan: 'Büyüktür',
+      lessThanOrEqual: 'Küçük Eşittir',
+      greaterThanOrEqual: 'Büyük Eşittir',
+      inRange: 'Aralık',
+      contains: 'İçeren',
+      notContains: 'İçermez',
+      endsWith: '-ile Başlayan',
+      startsWith: '-ile Biten',
+      filters: 'Filtreler',
+      noRowsToShow: 'Veri bulunamadı',
+    };
+
+    this.agGrid.gridOptions = {
+      getRowStyle: function (params) {
+        return { cursor: 'pointer' };
+      }
+    };
+  }
+
+  filterData() {
+    const queryLayer: esri.FeatureLayer = this.reportLayer;
+
+    let queryProperties: esri.QueryProperties;
+    queryProperties = {
+      where: '1=1',
+      outFields: ['*'],
+    };
+
+    this.addZonePerm(queryLayer, queryProperties);
+
+    this.createColDefs(queryLayer);
+
+    const dataSource: IDatasource = {
+      getRows: (params) => {
+        const queryParams = this.checkSortParameter(params, queryProperties);
+        const filteredParams = this.addFilterParameters(params.filterModel, queryParams);
+        this.whereQuery = filteredParams.where;
+
+        this.filterParams = filteredParams;
+        // queryLayer.queryFeatureCount(filteredParams).then(count => {
+        const start = params.startRow;
+        const end = params.endRow;
+        const num = end - start;
+
+        filteredParams.num = num;
+        filteredParams.start = start;
+
+        this.filterParams = filteredParams;
+        queryLayer.queryFeatures(filteredParams).then(res => {
+          const objectModel = res.features.map(feature => feature.attributes);
+          params.successCallback(objectModel);
+        });
+        // });
+      }
+    };
+    this.agGrid.api.setDatasource(dataSource);
+  }
+
+  checkSortParameter(params: IGetRowsParams, queryParameters: esri.QueryProperties): esri.QueryProperties {
+    const sortModels: Array<SortModel> = params.sortModel;
+    const queryParams = Object.assign({}, queryParameters);
+    if (sortModels.length > 0) {
+      const sortModel = sortModels[0];
+
+      const orderByFieldsDef = `${sortModel.colId} ${sortModel.sort}`;
+      queryParams.orderByFields = [orderByFieldsDef];
+    }
+
+    return queryParams;
+  }
+
+  addFilterParameters(filterModel: FilterModel, queryParams: esri.QueryProperties) {
+    const params = Object.assign({}, queryParams);
+    for (const param in filterModel) {
+      if (filterModel.hasOwnProperty(param)) {
+        const filteredFieldName = param;
+        const fieldDomain = this.getFieldDomain(filteredFieldName);
+        if (fieldDomain !== null) {
+          const fieldFilterModel = filterModel[param];
+          const queryConstructor = new QueryConstructor(fieldFilterModel, params.where, filteredFieldName, fieldDomain);
+
+          params.where = queryConstructor.getWhereQuery();
+        } else {
+          const fieldFilterModel = filterModel[param];
+          const queryConstructor = new QueryConstructor(fieldFilterModel, params.where, filteredFieldName);
+
+          params.where = queryConstructor.getWhereQuery();
+        }
+      }
+    }
+    return params;
+  }
+
+  getFieldDomain(fieldName: string): esri.Domain {
+    const foundField: esri.Field = this.reportLayer.fields.find(field => field.name === fieldName);
+    if (foundField !== undefined) {
+      return foundField.domain;
+    } else {
+      console.error('Field Domain Not Found');
+      return null;
+    }
+  }
+
+  createColDefs(layer: esri.FeatureLayer) {
+    const colDefs: Array<ColDef> = [];
+    layer.fields.forEach(field => {
+      if (field['get'] === undefined) {
+        const colDef = this.createColDef(field);
+
+        colDefs.push(colDef);
+      } else if (field.get('rAuthorized')) {
+        if (field.get('disabled') === true) {
+          return;
+        } else if (field.type === 'geometry') {
+          return;
+        }
+
+        const colDef = this.createColDef(field);
+        colDefs.push(colDef);
+      }
+    });
+    this.agGrid.api.setColumnDefs(colDefs);
+  }
+
+  createColDef(field: esri.Field) {
+    const colDef: ColDef = {};
+    if (field.type === 'string') {
+      colDef.headerName = field.alias;
+      colDef.colId = field.name;
+      colDef.field = field.name;
+      colDef.filter = 'agTextColumnFilter';
+      if (field.domain !== null) {
+        colDef.sortable = false;
+      }
+    } else if (field.type === 'date') {
+      colDef.headerName = field.alias;
+      colDef.colId = field.name;
+      colDef.field = field.name;
+      colDef.filter = 'agDateColumnFilter';
+      colDef.filterParams = {
+        browserDatePicker: true
+      };
+      this.createGetterForDate(colDef, field.name);
+    } else if (!(field.type === 'xml' || field.type === 'raster' || field.type === 'blob' || field.type === 'geometry')) {
+      if (field.domain !== null) {
+        colDef.filter = 'agTextColumnFilter';
+        colDef.sortable = false;
+        this.createGetterForDomain(colDef, field.name, field.domain);
+      } else {
+        colDef.filter = 'agNumberColumnFilter';
+      }
+      colDef.headerName = field.alias;
+      colDef.colId = field.name;
+      colDef.field = field.name;
+    } else {
+      console.error('Exception Data');
+    }
+    if (colDef.filter === 'agTextColumnFilter') {
+      colDef.filterParams = {
+        textFormatter: function (gridValue: string) {
+          const lowerCasedValue = gridValue.toLocaleLowerCase();
+          return lowerCasedValue;
+        }
+      };
+    }
+
+    return colDef;
+  }
+
+  createGetterForDate(coldef: ColDef, fieldName: string) {
+    coldef.valueGetter = (params) => {
+      if (params.data !== undefined) {
+        const fieldValue = params.data[fieldName];
+        if (fieldValue === null) {
+          return null;
+        }
+
+        return new Date(fieldValue).toLocaleDateString('tr-TR');
+      }
+    };
+  }
+
+  createGetterForDomain(coldef: ColDef, fieldName: string, domain: esri.Domain) {
+    coldef.valueGetter = (params) => {
+      if (params.data !== undefined) {
+        const fieldValue = params.data[fieldName];
+        if (fieldValue === null) {
+          return '';
+        }
+
+        if (domain.type === 'coded-value') {
+          const codedValueDomain: esri.CodedValueDomain = <esri.CodedValueDomain>domain;
+          const domainValue = codedValueDomain.getName(fieldValue);
+
+          return domainValue;
+        } else if (domain.type === 'codedValue') {
+          const codedValueDomain: esri.CodedValueDomain = <esri.CodedValueDomain>domain;
+          const foundCodedValueDomain = codedValueDomain.codedValues.find(codedValue => codedValue.code === fieldValue);
+
+          return foundCodedValueDomain.name;
+        } else {
+          console.error('Another Type Found');
+          return '';
+        }
+      }
+    };
+  }
+
+  addZonePerm(queryLayer: esri.FeatureLayer, queryProperties: esri.QueryProperties) {
+    if (queryLayer.get('zoneExpression') === '*') {
+      return;
+    }
+
+    if (queryLayer.get('zoneExpression') !== undefined) {
+      const zoneExpression = <string>queryLayer.get('zoneExpression');
+
+      if (zoneExpression.startsWith(' or')) {
+        const splittedZoneExpression = zoneExpression.substring(3);
+
+        queryProperties.where += ' and' + splittedZoneExpression;
+      } else {
+        if (zoneExpression.startsWith(' and')) {
+          if (queryProperties.where !== '') {
+            queryProperties.where += zoneExpression;
+          } else {
+            queryProperties.where = zoneExpression.substring(4);
+          }
+        } else {
+          if (queryProperties.where !== '') {
+            queryProperties.where += ' and' + zoneExpression;
+          } else {
+            queryProperties.where = zoneExpression;
+          }
+        }
+      }
+    } else {
+      const mapView = this.mapView;
+      const map = mapView.map;
+      const layers = map.layers;
+
+      const foundLayer = layers.find(layer => layer.get('zoneExpression') !== undefined);
+      if (foundLayer === undefined) {
+        return;
+      }
+
+      const zoneExpression = <string>foundLayer.get('zoneExpression');
+      if (zoneExpression.startsWith(' or')) {
+        const splittedZoneExpression = zoneExpression.substring(3);
+
+        queryProperties.where += ' and' + splittedZoneExpression;
+      } else {
+        if (zoneExpression.startsWith(' and')) {
+          if (queryProperties.where !== '') {
+            queryProperties.where += zoneExpression;
+          } else {
+            queryProperties.where = zoneExpression.substring(4);
+          }
+        } else {
+          if (queryProperties.where !== '') {
+            queryProperties.where += ' and' + zoneExpression;
+          } else {
+            queryProperties.where = zoneExpression;
+          }
+        }
+      }
+    }
+  }
+
+  exportDataAsExcel() {
+    const columns = this.agGrid.columnApi.getAllColumns();
+
+    const outFields = [];
+    const headerNames = [];
+    columns.forEach(column => {
+      if (column.isVisible()) {
+        headerNames.push(column.getColDef().headerName);
+        outFields.push(column.getColId());
+      }
+    });
+    const queryLayer: esri.FeatureLayer = this.reportLayer;
+    const filterParams: esri.QueryProperties = this.filterParams;
+
+    const progressDialog = this.dialog.openUnClosable(ProgressInfoComponent, 'Excel Çıktısı Hazırlanıyor', false, {}, false, 'light');
+    const httpOptions = new HttpHeaders({
+      'X-Content-Type-Options': 'nosniff',
+      'Authorization': sessionStorage.getItem('CBSMEBtoken')
+    });
+    const url = encodeURI(`${queryLayer.url}/${queryLayer.layerId}/query?where=${filterParams.where ? filterParams.where : '1=1'}`
+      + `&outFields=${outFields.toString()}&resultOffset=${filterParams.start.toString()}`
+      + `&resultRecordCount=${(filterParams.num * 10).toString()}&returnGeometry=false&f=json`);
+
+    const objectData = {
+      workSheetName: queryLayer.title,
+      headers: headerNames,
+      uri: url
+    };
+
+    this.http.post(environment.ws + 'Cbs/exportExcel', objectData, {
+      headers: httpOptions,
+      responseType: 'arraybuffer'
+    }).subscribe(res => {
+      const blob = new Blob([res], { type: ' application/vnd.openxmlformats-officedocument.spreadsheetml.sheet' });
+      const objectUrl = URL.createObjectURL(blob);
+      window.open(objectUrl);
+      progressDialog.close();
+    }, err => {
+      console.error('Something Went Wrong');
+      progressDialog.close();
+    });
+  }
+
+  onOpenSymbologyDialog() {
+    const columnApi: ColumnApi = this.agGrid.api['columnController'];
+
+    const columnsToExport = columnApi.getAllDisplayedColumns();
+
+    const fields = this.reportLayer.fields;
+    const layerFields = [];
+
+    columnsToExport.forEach(column => {
+      const foundField = fields.find(field => field.name === column.getColId());
+      if (foundField !== undefined) {
+        layerFields.push(foundField);
+      }
+    });
+
+    if (columnsToExport.length === 0) {
+      console.error('Katman olarak görüntülemek için tabloda listelemeniz gerekmektedir');
+    } else {
+      this.closeSymbologyBuilder();
+      this.symbologyBuilderDialog = this.dialog.open(LayerSymbologyComponent, 'Sembol Oluşturucu', true, {
+        data: {
+          whereQuery: this.whereQuery,
+          layer: this.reportLayer,
+          fields: layerFields,
+          mapView: this.mapView,
+        }
+      }, true, 'red-to-light');
+    }
+  }
+
+  closeSymbologyBuilder() {
+    if (this.symbologyBuilderDialog !== undefined) {
+      this.symbologyBuilderDialog.close();
+      this.symbologyBuilderDialog = undefined;
+    }
+  }
+
+  ngOnDestroy(): void {
+    this.closeSymbologyBuilder();
+  }
+}
