     });
   }
+
+  fillGUIAuthorization(userPermission: UserPermission) {
+    this.guiPermissions.push(this.isAuthorized(userPermission, 0));
+    this.guiPermissions.push(this.isAuthorized(userPermission, 1));
+  }
+
+  isAuthorized(userPermission: UserPermission, id: number) {
+    if (userPermission === undefined) {
+      return true;
+    }
+
+    const flattenPermItems = userPermission.flattenPermGUI[1];
+
+    if (flattenPermItems !== undefined) {
+      if (flattenPermItems.items.includes(id)) {
+        return true;
+      } else {
+        return false;
+      }
+    } else {
+      return false;
+    }
+  }
+
+  closeSymbologyBuilder() {
+    if (this.symbologyBuilderDialog !== undefined) {
+      this.symbologyBuilderDialog.close();
+      this.symbologyBuilderDialog = undefined;
+    }
+  }
 }
