+  forLastMovement(params: any, colDef: ColDef) {
+    const objectid = params.data['objectid'];
+    const rowIndex = params.rowIndex.toString();
+
+    const processMovementLayer = <esri.FeatureLayer>this.mapView.map.layers.find(layer => layer.title === 'Ä°slem_Hareket');
+    const movementLayer = <esri.FeatureLayer>this.mapView.map.layers.find(layer => layer.title === 'Hareket');
+    const queryProp: esri.QueryProperties = {
+      where: `i_objectid=${objectid}`,
+      outFields: ['h_durumu']
+    };
+
+    processMovementLayer.queryFeatures(queryProp).then(response => {
+
+      const domainCode = response.features[0].attributes['h_durumu'];
+      const durumuField = movementLayer.fields.find(field => (field.name === 'durumu' || field.name === 'h_durumu'));
+      if (durumuField !== null) {
+        const domain = durumuField.domain['codedValues'].find(codeValue => codeValue.code === Number(domainCode));
+
+        const rowNode = this.agGrid.api.getRowNode(rowIndex);
+        rowNode.setDataValue('lastMovement', domain.name);
+        this.agGrid.api.refreshCells();
+      }
+    });
+  }
