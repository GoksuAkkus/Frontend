+  setKoyMahalleleri(bucaklar: Array<Bucak>) {
+    this.dataArray[0].bucaklar = bucaklar;
+    let bucakAdi = '';
+    let koyAdi = '';
+    let koyMahallesiAdi = '';
+    let koyMahallesiKodu = 0;
+    let mahalleTipi = '';
+    bucaklar.forEach(bucak => {
+      bucakAdi = bucak.bucakAdi;
+      bucak.bagliKoyListesi.forEach(koyler => {
+        koyAdi = koyler.koyAdi;
+        koyler.bagliMahalleListesi.forEach(koyMahallesi => {
+          koyMahallesiAdi = koyMahallesi.mahalleAdi;
+          koyMahallesiKodu = koyMahallesi.kod;
+          mahalleTipi = koyMahallesi.mahalleTipKod['aciklama'];
+          if (koyMahallesiAdi === null) {
+            koyMahallesiAdi = 'KÖYÜN KENDİSİ';
+          }
+          const ad = bucakAdi + ',' + koyAdi + ',' + koyMahallesiAdi + ', ' + mahalleTipi;
+
+          const yeniMahalle: Mahalle = { ad: ad, kod: koyMahallesiKodu };
+          this.dataArray[0].mahalleler.push(yeniMahalle);
+        });
+      });
+    });
+  }
