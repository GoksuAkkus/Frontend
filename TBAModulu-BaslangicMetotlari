+  addOrRemoveDynamicFieldsForAtolye(count: number, recentFormValues?: Object) {
+    const atolyeTuruFieldName = 'atolye_turu';
+    const atolyeIslemFieldName = 'atolye_islem';
+
+    let atolyeTuruValue: any;
+    let atolyeIslemValue: any;
+
+    let startIndex = 1;
+    const filteredFormControls = this.moduleFormData
+      .filter(formControl => formControl.name.includes('atolye_turu'));
+
+    const dynamicFieldLength = filteredFormControls.length;
+
+    if (dynamicFieldLength === count) {
+      return;
+    } else if (dynamicFieldLength > count) {
+      const deleteIndex = count + 1;
+      this.removeDynamicFieldsForAtolye(deleteIndex, dynamicFieldLength);
+      startIndex = dynamicFieldLength;
+    } else if (dynamicFieldLength < count) {
+      startIndex = count - (count - dynamicFieldLength) + 1;
+    }
+    for (let i = startIndex; i <= count; i++) {
+      const atolyeTuruField = atolyeTuruFieldName + i;
+      const atolyeIslemField = atolyeIslemFieldName + i;
+
+      const atolyeTuruFormData = <FormData>{
+        alias: `Atolye ${i} Türü`,
+        name: atolyeTuruField,
+        type: 'localDomain',
+        required: true,
+        chosen_values: [],
+        domain_values: this.getAtolyeTuruDomains()
+      }
+      const atolyeIslemFormData = <FormData>{
+        alias: ` Atölye ${i} İşlem`,
+        name: atolyeIslemField,
+        type: 'localDomain',
+        required: true,
+        chosen_values: [],
+        domain_values: this.getAtolyeIslemDomains()
+      }
+      const atolyeTuruFormControlName = atolyeTuruFormData.name;
+      const atolyeIslemFormControlName = atolyeIslemFormData.name;
+      const atolyeTuruFormControlType = atolyeTuruFormData.type;
+      const atolyeIslemFormControlType = atolyeIslemFormData.type;
+      const required = true;
+      if (recentFormValues !== undefined) {
+        atolyeTuruValue = recentFormValues[atolyeTuruFormControlName];
+        atolyeIslemValue = recentFormValues[atolyeIslemFormControlName];
+      }
+      if (this.processState === 0) {
+        const atolyeTuruValueArray = (<string>recentFormValues[atolyeTuruFormControlName]).split(',');
+        const atolyeIslemValueArray = (<string>recentFormValues[atolyeIslemFormControlName]).split(',');
+
+        atolyeTuruValue = atolyeTuruValueArray[i - 1];
+        atolyeIslemValue = atolyeIslemValueArray[i - 1];
+      }
+
+      const atolyeTuruFormControl = this.createFormControl(atolyeTuruFormControlName, atolyeTuruFormControlType
+        , required, atolyeTuruValue);
+
+      this.formGroup.addControl(atolyeTuruFormControlName, atolyeTuruFormControl);
+
+      const atolyeIslemFormControl = this.createFormControl(atolyeIslemFormControlName, atolyeIslemFormControlType, required,
+        atolyeIslemValue);
+      this.formGroup.addControl(atolyeIslemFormControlName, atolyeIslemFormControl);
+
+      if (this.processState === 0) {
+        this.formGroup.controls[atolyeTuruFormControlName].disable();
+        this.formGroup.controls[atolyeIslemFormControlName].disable();
+      }
+
+      this.moduleFormData.push(atolyeTuruFormData);
+      this.moduleFormData.push(atolyeIslemFormData);
+    }
 
+  }
+  getAtolyeTuruDomains() {
+    return [{
+      name: 'Hizmet binası',
+      value: 'Hizmet binası',
+    },
+    {
+      name: 'Derslik',
+      value: 'Derslik'
+    },
+    {
+      name: 'Spor Salonu',
+      value: 'Spor Salonu'
+    },
+    {
+      name: 'Konferans Salonu',
+      value: 'Konferans Salonu'
+    },
+    {
+      name: 'Yemekhane',
+      value: 'Yemekhane'
+    }];
+  }
+  getAtolyeIslemDomains() {
+    return [{
+      name:'Tadilat ve Mal Alımı',
+      value: 'Tadilat ve Mal Alımı',
+    },
+    {
+      name:'Sadece Tadilat',
+      value: 'Sadece Tadilat',
+    },
+    {
+      name:'Sadece Mal Alımı',
+      value: 'Sadece Mal Alımı',
+    }];
+  }
