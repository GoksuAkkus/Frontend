+  removeDynamicFieldsForAtolye(startingIndex: number, dynamicFieldLength: number) {
+    const atolyeTuruFieldName = 'atolye_turu';
+    const atolyeIslemFieldName = 'atolye_islem';
+
+    const moduleFormData = this.moduleFormData;
+
+    for (let i = startingIndex; i <= dynamicFieldLength; i++) {
+      const atolyeTuruField = atolyeTuruFieldName + i;
+      const atolyeIslemField = atolyeIslemFieldName + i;
+
+      this.formGroup.removeControl(atolyeTuruField);
+      this.formGroup.removeControl(atolyeIslemField);
+    }
+
+    const firstAtolyeTuruField = atolyeTuruFieldName + startingIndex;
+
+    const atolyeTuruIndex = moduleFormData.findIndex(formData => formData.name === firstAtolyeTuruField);
+    moduleFormData.length = atolyeTuruIndex;
+  }
 
