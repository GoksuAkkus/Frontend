+                      let attachmentIDs = ''
+                      if (attachmentInfos.length > 0) {
+                        attachments[0]['attachmentInfos'].forEach(attachment => {
+                          attachmentIDs += attachment['id'].toString();
+                          attachmentIDs += ','
+                        });
+                        attachmentIDs = attachmentIDs.slice(0, attachmentIDs.length - 1);
+                      }
+
+                      attachmentForm.append('attachmentIds', attachmentIDs);
+                      attachmentForm.append('rollbackOnFailure', 'true');
+                      attachmentForm.append('f', 'json');
+                      attachmentForm.append('token', sessionStorage.getItem('CBSArcToken'));
+                      this.http.post(deleteAttachmentURL, attachmentForm).subscribe(deleteResponse => {
+                        if (attachmentInfos.length === 0) {
+                          deleteResponse = { deleteAttachmentResults: [{ success: true }] };
+                          isAllOk = true;
+                        } else {
+                          deleteResponse['deleteAttachmentResults'].forEach(deleteAttachmentResult => {
+                            let isAllOk = true;
+                            if(!deleteAttachmentResult['success']) {
+                              isAllOk = false;
+                            }
+                          });
+                        }
+                        
