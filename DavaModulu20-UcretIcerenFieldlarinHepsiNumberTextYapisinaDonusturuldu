+                            let status = true;
+                            addAttachmentData.find(data => {
+                              const success = data['addAttachmentResult'].success;
+                              if (!success) {
+                                status = false;
+                              }
+                            });
+
+                            if (addAttachmentData !== undefined) {
+                              if (status !== true) {
+                                console.error('Attachment Can Not Be Added.');
+                                this.notify.open('Dosyalar Güncellenirken Bir Hata Oluştu', 'warning', 3);
+                              } else {
+                                this.notify.open('Dosyalar Başarıyla Güncellendi', 'success', 3);
+                              }
+                              this.dialog.close(true);
+                            } else {
