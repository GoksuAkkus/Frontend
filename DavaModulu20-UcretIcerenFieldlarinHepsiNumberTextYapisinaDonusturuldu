+                              this.dialog.close(true);
+                            }
+                          });
+                        } else {
+                          this.notify.open('Çoklu Hareket Güncellerken Hata Oluştu', 'danger', 5);
+                        }
+                      });
+                    }
+                  });
+                }
+              });
