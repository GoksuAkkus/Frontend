+      const talepTuru = this.moduleData['talep_turu'];
+      const domainValues = movementData.field.domain_values;
+      const extractDomainValuesForCustomize = [8, 10, 11, 12, 13, 14, 15, 16, 17]; // Talep Türü 10 ve 12 Olanların Hareketlerinden Bunlar Çıkmalı
+      const extractDomainValuesForDefault = [3]; // Talep Türü 11 Olanların Hareketlerinden Bunlar Çıkmalı
+      if (talepTuru === 10 || talepTuru === 12) {
+        for (let i = 0; i < extractDomainValuesForCustomize.length; i++) {
+          const extractDomain = extractDomainValuesForCustomize[i];
+          const foundIndex = domainValues.findIndex(extDomain => extDomain.value === extractDomain);
+          if (foundIndex !== -1) {
+            domainValues.splice(foundIndex, 1);
+          } else {
+            continue;
+          }
+        }
+      } else {
+        for (let i = 0; i < extractDomainValuesForDefault.length; i++) {
+          const extractDomain = extractDomainValuesForDefault[i];
+          const foundIndex = domainValues.findIndex(extDomain => extDomain.value === extractDomain);
+          if (foundIndex !== -1) {
+            domainValues.splice(foundIndex, 1);
+          } else {
+            continue;
+          }
+        }
+      }
+    }
