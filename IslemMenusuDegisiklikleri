+    // GAGA
+    // Hiç hareket olmadığı durumda (this.features boş ise) Plan işleminde domainler 6 ve 7 değilse son 3 domain (9,36,37) silinmeli.
+    // hareketi olup alt konusu 6 ve 7yse gelen movementdata zaten ona göre gelir .
+    if (this.features.length === 0 && this.moduleData['islem_turu'] === 5) {
+      const altKonu = this.moduleData['alt_konu'];
+      // talep turu özel okul dönüşümü ve kamu yararı (islem_turu= 5 talep turu= 4) ise de son 3 domain gelmeli
+      if (altKonu !== 6 && altKonu !== 7 && this.moduleData['talep_turu'] !== 4 && this.moduleData['talep_turu'] !== 5) {
+        movementData.field.domain_values = movementData.field.domain_values.slice(0, 7);
