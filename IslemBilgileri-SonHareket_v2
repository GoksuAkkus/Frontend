@@ -148,6 +155,59 @@ export class ModuleListComponent implements OnInit, OnDestroy {
     this.agGrid.context = { componentParent: this };
   }
 
+  initializeLastMovement(whereField: string) {
+    const movementLayer = <esri.FeatureLayer>this.mapView.map.layers.find(layer => layer.title === 'Hareket');
+    const processMovementLayer = <esri.FeatureLayer>this.mapView.map.layers.find(layer => layer.title === 'İslem_Hareket');
+    const durumuField = movementLayer.fields.find(field => (field.name === 'durumu' || field.name === 'h_durumu'));
+    if (durumuField !== null) {
+      this.hDurumuDomains = durumuField.domain['codedValues'];
+    }
+
+    const value = this.emitObject.rowClickData[`i_${whereField}`];
+    const queryProp: esri.QueryProperties = {
+      where: `i_${whereField}=${value}`,
+      returnGeometry: false,
+      outFields: ['i_objectid', 'h_durumu']
+    };
+
+    processMovementLayer.queryFeatures(queryProp).then(response => {
+      response.features.forEach(feature => {
+        const rowInfo = new EmitObjectForLastMovement();
+        const attributes = feature.attributes;
+        const hDurumu = attributes['h_durumu'];
+        if (hDurumu === null) {
+          rowInfo.lastMovement = '';
+        } else {
+          const codeValue = this.hDurumuDomains.find(domain => domain.code === Number(hDurumu));
+          rowInfo.lastMovement = codeValue['name'];
+        }
+        rowInfo.objectId = attributes['i_objectid'];
+        this.rowsInfoArray.push(rowInfo);
+      });
+    });
+  }
+
+  filterForLastMovement(filterModel: any) {
+    const filterParam = filterModel['lastMovement']['filter'].toLocaleLowerCase();
+    const newRows = [];
+    this.lastMovementFilterArray = [];
+
+    this.rowsInfoArray.forEach(rowInfo => {
+      if (rowInfo.lastMovement.toLocaleLowerCase().includes(filterParam)) {
+        this.lastMovementFilterArray.push(rowInfo);
+      }
+    });
+
+    this.lastMovementFilterArray.forEach(element => {
+      const objectId = element.objectId;
+      const foundRow = this.allRows.find(row => row['objectid'] === objectId);
+      if (foundRow) {
+        newRows.push(foundRow);
+      }
+    });
+    return newRows;
+  }
+
   setDataSourceOfGrid(whereField: string, whereValue: string | number) {
     if (isNaN(Number(whereValue))) {
       whereValue = `'${whereValue}'`;
