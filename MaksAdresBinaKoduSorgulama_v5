+  addFilterParametersForGelGit(queryParams: esri.QueryProperties, gelgitSayiFilterModel: any, gelgitTarihFilterModel: any) {
+    const params = Object.assign({}, queryParams);
+
+    if(gelgitSayiFilterModel) {
+      const sayiQuery = new QueryConstructor(gelgitSayiFilterModel, params.where, 'i_sayi');
+      params.where = sayiQuery.getWhereQuery();
+      const gidenSayiQuery = new QueryConstructor(gelgitSayiFilterModel, params.where, 'h_giden_sayi');
+      params.where = gidenSayiQuery.getWhereQuery();
+      const gelenSayiQuery = new QueryConstructor(gelgitSayiFilterModel, params.where, 'h_gelen_sayi');
+      params.where = gelenSayiQuery.getWhereQuery();
+    }
+
+    if(gelgitTarihFilterModel) {
+      const tarihQuery = new QueryConstructor(gelgitTarihFilterModel, params.where, 'i_bitis_tarihi');
+      params.where = tarihQuery.getWhereQuery();
+      const gidenTarihQuery = new QueryConstructor(gelgitTarihFilterModel, params.where, 'h_giden_tarih');
+      params.where = gidenTarihQuery.getWhereQuery();
+      const gelenTarihQuery = new QueryConstructor(gelgitTarihFilterModel, params.where, 'h_gelen_tarih');
+      params.where = gelenTarihQuery.getWhereQuery();
+    }
+
+    return params;
+  }
