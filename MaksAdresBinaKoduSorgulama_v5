+
+  get mapView() {
+    return this._mapView;
+  }
+
+  subscription: Subscription;
+  dataArray: Array<Array<any>> = [];
+  filterArray: Array<Array<any>> = [];
+  denemeArray: Array<MaksData> = [new MaksData()];
+  selectedDataArray: Array<any> = [];
+  progressInfoDialogRef: DialogRef;
+  _mapView: esri.MapView;
+  ilArray: Array<Il> = [];
+  userPerm: UserPermission;
+  districtPerm: Array<DistrictPermTKGM> = [];
+
+  constructor(private webdataservice: WebDataService, private dialogService: DialogService, private notify: NotificationService,
+    private auth: AuthService) { }
+
+  ngOnInit() {
+
+    this.userPerm = this.auth.userPerm;
+    const mapView = this._mapView;
+    const cityPerm: Array<string> = [];
+
+    if (this.userPerm !== undefined) {
+      this.auth.userPerm['permZoneCityTKGM'].map(cityName => {
+        cityPerm.push(cityName.toLocaleUpperCase());
+      });
+
+      this.districtPerm = this.auth.userPerm['permZoneDistrictTKGM'];
+
+      if (this.districtPerm.length > 0) {
+        this.districtPerm.map(perm => {
+          if (!cityPerm.some(city => city === perm['city'].toLocaleUpperCase())) {
+            cityPerm.push(perm['city'].toLocaleUpperCase());
+          }
+        });
+      }
+
+      const permittedCities = [];
+
+      const ilLayer: esri.FeatureLayer = <esri.FeatureLayer>mapView.map.allLayers.find(layer => layer.title === 'İl');
+      const ilLayerQuery: esri.QueryProperties = {
+        where: '1=1',
+        orderByFields: ['ad asc'],
+        outFields: ['ad', 'uavtil'],
+        groupByFieldsForStatistics: ['ad', 'uavtil'],
+        returnGeometry: false,
+      };
+
+      ilLayer.queryFeatures(ilLayerQuery).then(res => {
+        const features = res.features;
+        for (let i = 0; i < features.length; i++) {
+          const il = new Il();
+          il.ad = features[i].attributes['ad'];
+          il.uavtil = features[i].attributes['uavtil'];
+          this.ilArray.push(il);
+        }
+      });
+    }
+  }
+
+  dispatchMaksAdresBinaKoduSearchClicked(event: Event) {
+    this.maksAdresBinaKoduClicked.emit(event);
+  }
+
+  onToggleDropdown() {
+    const inputElement: HTMLInputElement = <HTMLInputElement>event.target;
+    const formGroup = inputElement.parentElement;
+    const dropDownImg = inputElement.parentElement.lastElementChild;
+    const selectContainer: HTMLElement = <HTMLElement>formGroup.nextElementSibling;
+
+    if (dropDownImg.classList.contains('r-90')) {
+      selectContainer.style.display = 'none';
+    } else {
+      selectContainer.style.display = 'flex';
+      this.filterArray[0] = this.dataArray[0];
+    }
+
+    dropDownImg.classList.toggle('r-90');
+  }
+
+  onInput() {
+    const inputElement: HTMLInputElement = <HTMLInputElement>event.target;
+    const inputContainer = inputElement.parentElement;
+    const formElement: HTMLFormElement = <HTMLFormElement>inputContainer.parentElement;
+    this.resetFormData(formElement, inputContainer);
+
+    const inputValue = inputElement.value;
+
+    const index = this.getIndexElement(inputContainer);
+
+    const searchFilterObject: SearchFilterObject = {
+      array: this.dataArray[index],
+      filterKey: inputValue,
+      isNumber: false
+    };
+
+    if (index < 3) {
+      this.filterArray[index] = this.filterSearchData(searchFilterObject);
+    } else {
+      searchFilterObject.isNumber = true;
+      this.filterArray[index] = this.filterSearchData(searchFilterObject);
+      this.selectedDataArray[index] = inputValue;
+    }
+  }
+
+  onSelect(event: Event) {
+
+    const selectedElement: HTMLElement = <HTMLElement>event.target;
+    const selectContainer: HTMLElement = selectedElement.parentElement;
+    const form: HTMLFormElement = <HTMLFormElement>selectContainer.parentElement;
+
+    const inputContainer: HTMLElement = <HTMLElement>selectContainer.previousElementSibling;
+    const selectInput: HTMLInputElement = <HTMLInputElement>inputContainer.firstElementChild;
+
+    const selectContainerNextElement: HTMLElement = <HTMLElement>selectContainer.nextElementSibling;
+    let notifyMessageTitle;
+
+    if (selectContainerNextElement !== null) {
+      const nextElementInput: HTMLInputElement = <HTMLInputElement>selectContainerNextElement.firstElementChild;
+      notifyMessageTitle = nextElementInput.name;
+    }
+
+    this.resetFormData(form, inputContainer);
+
+    selectInput.value = '';
+    selectInput.blur();
+
+    const selectedElementTitle = selectedElement.innerText;
+    selectInput.value = selectedElementTitle;
+    selectInput.blur();
+
+    const inputArray = [];
+
+    const currentTarget = event.currentTarget;
+    const selectName = selectInput.name;
+    const kod = currentTarget['id'];
+    switch (selectName) {
+      case 'iller': {
+        inputArray[0] = kod;
+        break;
+      }
+      case 'ilceler': {
+        inputArray[1] = kod;
+        break;
+      }
+      case 'mahalleler': {
+        inputArray[2] = kod;
+        break;
+      }
+      case 'csbmler': {
+        inputArray[3] = kod;
+        break;
+      }
+      case 'diskapiNolar': {
+        inputArray[4] = kod;
+        break;
+      }
+    }
+    const dataService = this.webdataservice;
+
+    // const deneme = dataService.getIlcelerFromMAKS(23);
+    // console.log(deneme);
+
+    const index = this.getIndexElement(inputContainer);
+
+    dataService.getIlcelerFromMAKS(inputArray[0]).subscribe(res => {
+      this.denemeArray[0].ilceler = res['ilceler'];
+    });
+    dataService.getMahallelerFromMAKS(inputArray[1]).subscribe(res => {
+      this.denemeArray[0].mahalleler = res['mahalleler'];
+      console.log(res.mahalleler);
+    });
+    dataService.getCsbmlerFromMAKS(inputArray[2]).subscribe(res => {
+      this.denemeArray[0].csbmler = res['csbmler'];
+    });
+    dataService.getBinalarFromMAKS(inputArray[3]).subscribe(res => {
+      this.denemeArray[0].binalar = res['binalar'];
+    });
+
+    if (this.subscription !== undefined) {
+      this.subscription.unsubscribe();
+    }
+
+
+  }
+
+  getGeometryFromTKGM(search_query) {
+    this.webdataservice.getGeometryFromTKGM(search_query).subscribe(result => {
+      const tkgmGeometryObject: TKGMGeometryObject = {
+        rings: result['rings'],
+      };
+      this.gotoSelectedObject.emit(tkgmGeometryObject);
+      this.closeProgressDialog();
+    }, error => {
+      this.closeProgressDialog();
+      this.notify.open('Geometri yüklenirken hata oluştu!', 'danger', 5);
+    });
+  }
+
+  setPermittedDistrictsArray() {
+    const selectedCity = this.dataArray[0].find(city => city.id === parseInt(this.selectedDataArray[0], 0));
+
+    if (this.districtPerm.length > 0) {
+      const permittedDistricts = [];
+
+      this.districtPerm.map(perm => {
+        if (selectedCity.ad === perm['city'].toLocaleUpperCase()) {
+          permittedDistricts.push(perm['district'].toLocaleUpperCase());
+        }
+      });
+
+      if (permittedDistricts.length > 0) {
+        this.filterArray[1] = [];
+        permittedDistricts.map(dist => {
+          const district = this.dataArray[1].find(item => item.ad === dist);
+          this.filterArray[1].push(district);
+        });
+      }
+    }
+  }
+
+  filterSearchData(searchFilterObject: SearchFilterObject) {
+    if (searchFilterObject.array && searchFilterObject.array.length !== 0) {
+      return searchFilterObject.array.filter(item => searchFilterObject.isNumber ? item.includes(searchFilterObject.filterKey) :
+        item.ad.includes(searchFilterObject.filterKey.toLocaleUpperCase()));
+    }
+  }
+
+  getIndexElement(inputContainerElement: HTMLElement): number {
+    let previousElementSibling = inputContainerElement.previousElementSibling;
+    let index = 0;
+    while (previousElementSibling !== null) {
+      if (!previousElementSibling.classList.contains('select-options')) {
+        index++;
+      }
+      previousElementSibling = previousElementSibling.previousElementSibling;
+    }
+
+    return index;
+  }
+
+  resetFormData(form: HTMLFormElement, inputContainerElement: HTMLElement) {
+    const formElements: HTMLCollectionOf<HTMLFormElement> = <HTMLCollectionOf<HTMLFormElement>>form.elements;
+    const formElementsLength = formElements.length;
+    const index = this.getIndexElement(inputContainerElement);
+
+    for (let i = index + 1; i < formElementsLength; i++) {
+      const inputElement: HTMLFormElement = <HTMLFormElement>formElements[i];
+      if (inputElement !== undefined) {
+        inputElement.value = '';
+        this.filterArray[i] = [];
+      }
+    }
+  }
+
+  closeProgressDialog() {
+    if (this.progressInfoDialogRef !== undefined) {
+      this.progressInfoDialogRef.close();
+      this.progressInfoDialogRef = undefined;
+    }
+  }
+}
