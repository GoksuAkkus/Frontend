+  getYapiBilgileri(binaSiraNo: number) {
+    // this.webDataService.getBinaSiraNoileInfo(binaSiraNo).subscribe(dd => {
+    //   this.infoData = dd;
+    // });
+    let yapisalBilgiGraphic :esri.Graphic;
+    let yapisalDurumGraphic :esri.Graphic;
+    let yapiEnerjiGraphic :esri.Graphic;
+
+    const yapisalBilgiLayer = <esri.FeatureLayer>this.mapView.map.layers.find(layer => layer.title === 'Yapısal Bilgi');
+    const yapisalDurumLayer = <esri.FeatureLayer>this.mapView.map.layers.find(layer => layer.title === 'Yapısal Durum');
+    const yapiEnerjiLayer = <esri.FeatureLayer>this.mapView.map.layers.find(layer => layer.title === 'Yapı Enerji');
+    const where = `bina_sira_no  = ${binaSiraNo} `;
+
+    const yapisalBilgiAuthorizedFields = yapisalBilgiLayer.fields.filter(field => field.get('rAuthorized') === true);
+    const yapisalDurumAuthorizedFields = yapisalDurumLayer.fields.filter(field => field.get('rAuthorized') === true);
+    const yapiEnerjiAuthorizedFields = yapiEnerjiLayer.fields.filter(field => field.get('rAuthorized') === true);
+
+    const yapisalBilgiOutfields = yapisalBilgiAuthorizedFields.map(authorizedField => authorizedField.name);
+    const yapisalDurumOutfields = yapisalDurumAuthorizedFields.map(authorizedField => authorizedField.name);
+    const yapiEnerjiOutfields = yapiEnerjiAuthorizedFields.map(authorizedField => authorizedField.name);
+
+    const yapisalBilgiQueryParams: esri.QueryProperties = {
+      where: where,
+      outFields: yapisalBilgiOutfields
+    };
+    const yapisalDurumQueryParams: esri.QueryProperties = {
+      where: where,
+      outFields: yapisalDurumOutfields
+    };
+    const yapiEnerjiQueryParams: esri.QueryProperties = {
+      where: where,
+      outFields: yapiEnerjiOutfields
+    };
+
+    yapisalBilgiLayer.queryFeatures(yapisalBilgiQueryParams).then(yapisalBilgiResponse => {
+      yapisalDurumLayer.queryFeatures(yapisalDurumQueryParams).then(yapisalDurumResponse => {
+        yapiEnerjiLayer.queryFeatures(yapiEnerjiQueryParams).then(yapiEnerjiResponse => {
+           yapisalBilgiGraphic = yapisalBilgiResponse.features[0];
+           yapisalDurumGraphic = yapisalDurumResponse.features[0];
+           yapiEnerjiGraphic = yapiEnerjiResponse.features[0];
+
+          const mainInformation = undefined;
+
+          this.infoData = {
+            mainInformation: mainInformation,
+            extraInformation: {
+              yapisal_bilgi: {
+                graphic: yapisalBilgiGraphic,
+                tabName: 'Yapısal Bilgi'
+              },
+              yapisal_durum: {
+                graphic: yapisalDurumGraphic,
+                tabName: 'Deprem'
+              },
+              yapi_enerji: {
+                graphic: yapiEnerjiGraphic,
+                tabName: 'Enerji'
+              }
+            },
+
+          };
+        });
+      });
+    });
