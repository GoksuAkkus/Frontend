+  setDataSourceForYapi(colDefs: ColDef[], tableName: string) {
+    this.agGrid.api.setColumnDefs(colDefs);
+
+    const dataSourceYapi = {
+      getRows: (params) => {
+        const start = params.startRow;
+        const end = params.endRow;
+        const count = end - start;
+        let whereQuery = '';
+        let where = '1=1';
+
+        if (Object.keys(params.filterModel).length !== 0) {
+
+          const filterModel = params.filterModel;
+
+          for (const element in filterModel) {
+
+            if (element) {
+              const filterModelElement = filterModel[element];
+
+              if (filterModelElement.operator) {
+                const operator = filterModelElement.operator;
+                whereQuery += this.filterYapi(element, filterModelElement, operator) + ' AND ';
+              } else {
+                whereQuery += this.filterYapi(element, filterModelElement) + ' AND ';
+              }
+            }
+          }
+          const deleteString = whereQuery.length - 4;
+          whereQuery = whereQuery.substring(0, deleteString);
+        }
+        if (whereQuery !== '') {
+          where = whereQuery;
+        }
+        this.http.get<number>(environment.yapiURL + 'grid/countDataForYapi', {
+          params: {
+            where: where,
+            layerName: tableName
+          }
+        }).subscribe(countData => {
+          const istek = this.http.get<Array<any>>(environment.yapiURL + 'gridDetayliArama', {
+            params: {
+              layerName: tableName,
+              start: start.toString(),
+              count: count.toString(),
+              where: where
+            }
+          }).subscribe(gridData => {
+            params.successCallback(gridData, countData);
+          });
+        });
+      }
+    } as IDatasource;
+
+    this.agGrid.api.setDatasource(dataSourceYapi);
+  }
