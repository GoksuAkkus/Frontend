+  queryFeatureDataForEgitimBolgeleri(authorizedFields: Array<esri.Field>, uniqueFieldName: string,
+    uniqueID: number, selectedLayer: esri.FeatureLayer) {
+    const outFields = authorizedFields.map(authorizedField => authorizedField.name);
+    const where = `${uniqueFieldName} = ${uniqueID}`;
+
+    const queryParams: esri.QueryProperties = {
+      where: where,
+      outFields: outFields,
+      returnGeometry: true,
+    };
+
+    const layerQuery = selectedLayer.queryFeatures(queryParams).then(responseObject => {
+      const attributes = responseObject.features[0].attributes;
+      const definitionExp = attributes['bolge_tipi'];
+
+      // tslint:disable-next-line:max-line-length
+      const foundLayer = this.mapView.map.layers.find(layer => layer['definitionExpression'] === `bolge_tipi=${definitionExp}`) as esri.FeatureLayer;
+      responseObject.features[0].layer = foundLayer;
+      this.dialogRef.emit(responseObject);
+    });
+  }
+
