+export class MaksData {
+    ilceler: Array<Ilce>;
+    mahalleler: Array<Mahalle>;
+    csbmler: Array<Csbm>;
+    binalar: Array<Bina>;
+    geometry: MaksGeometry;
+    error: Error;
+}
+
+export class Il {
+    ad: string;
+    uavtil: number;
+}
+
+export interface Ilce {
+    ad: string;
+    ilKod: number;
+    kod: number;
+}
+
+export interface Mahalle {
+    ad: string;
+    kod: number;
+}
+
+export interface Csbm {
+    ad: string;
+    ilKodu: number;
+    ilceKodu: number;
+    mahalleKodu: number;
+    kod: number;
+}
+
+export interface Bina {
+
+    ilKodu: number;
+    ilceKodu: number;
+    mahalleKodu: number;
+    csbmKodu: number;
+    binaKodu: number;
+
+    ada: number;
+    binaDurum: string;
+    binaNo: number;
+    binaNumaratajTipi: string;
+    binaYapiTipi: string;
+
+    disKapiNo: string;
+
+}
+
+export interface MaksGeometry {
+
+    rings: Array<Array<number[]>>;
+}
+
+export interface Error {
+    errorCode: number;
+    errorDescription: string;
+}
