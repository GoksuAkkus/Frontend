+  createEgitimBolgesiLayer(url: string) {
+    const featureLayerProps = <esri.FeatureLayerProperties>{
+      url: url
+    };
+
+    const featureLayer = new this.FeatureLayerConstructor(featureLayerProps);
+
+    return featureLayer;
+  }
+
+  getGroupByKurum() {
+    const url = 'https://cbs.meb.gov.tr/server/rest/services/MEB_IEDB/bolge_olustur/FeatureServer/2/query?where=1=1&relationParam=' +
+      '&outFields=*&returnGeometry=true&groupByFieldsForStatistics=kurum_kodu&outStatistics=[%20{%20%22statisticType%22:%20%22count%22,' +
+      '%20%22onStatisticField%22:%20%22kurum_kodu%22,%20%22outStatisticFieldName%22:%20%22count%22%20}%20]&f=json';
+    const bolgeListesiLayerURI = 'https://cbs.meb.gov.tr/server/rest/services/MEB_IEDB/bolge_olustur/FeatureServer/2';
+    const bolgeLayerURI = 'https://cbs.meb.gov.tr/server/rest/services/MEB_IEDB/bolge_olustur/FeatureServer/1';
+    const featureLayer = this.createEgitimBolgesiLayer(bolgeListesiLayerURI);
+    const zoneLayer = this.createEgitimBolgesiLayer(bolgeLayerURI);
+
+    this.http.get(url + '?token=' + sessionStorage.getItem('CBSArcToken')).subscribe(res => {
+      const features = res['features'] as Array<any>;
+      this.createBolge(features, featureLayer, zoneLayer, 0);
+    });
+  }
+
+  createBolge(features: Array<any>, featureLayer: esri.FeatureLayer, zoneLayer: esri.FeatureLayer, index: number) {
+    const feature = features[index];
+
+    if (!feature) {
+      return;
+    }
+
+    const kurumKodu = feature['kurum_kodu'];
+    const count = feature['count'];
+    const requestCount = Math.ceil(count / 1000);
+
+    const promises = [] as Array<any>;
+
+    for (let j = 0; j < requestCount; j++) {
+      const queryParams = {
+        where: `kurum_kodu=${kurumKodu}`,
+        num: 1000,
+        start: count * 1000
+      } as esri.QueryProperties;
+
+      const requestPromise = featureLayer.queryFeatures(queryParams);
+      promises.push(requestPromise);
+    }
+
+    Promise.all(promises).then((res: Array<esri.FeatureSet>) => {
+      const totalFeatures = [];
+      res.forEach(featureResult => {
+        totalFeatures.push(...featureResult.features);
+      });
+
+      const zone = this.createHull(totalFeatures);
+      this.postZoneGeometry(zoneLayer, zone).then(zoneRes => {
+        console.log(zoneRes);
+        this.createBolge(features, featureLayer, zoneLayer, index + 1);
+      });
+    });
+  }
+
+  createHull(features: Array<esri.Feature>) {
+    const points = [];
+    features.forEach(feature => {
+      const rings = feature['geometry']['rings'] as Array<any>;
+
+      rings[0].forEach(ring => {
+        points.push([ring[0], ring[1]]);
+      });
+    });
+
+    const zone = hull(points, 0.0005);
+
+    return zone;
+  }
+
+  postZoneGeometry(zoneLayer: esri.FeatureLayer, zone: any) {
+    const polygon = new this.polygonModule({ rings: [zone] });
+    const polygonGraphic = new this.graphicModule({ geometry: polygon });
+    const applyEdits = {
+      addFeatures: [polygonGraphic]
+    } as esri.FeatureLayerApplyEditsEdits;
+    return zoneLayer.applyEdits(applyEdits);
+  }
+
