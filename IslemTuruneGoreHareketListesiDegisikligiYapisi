@@ -155,6 +179,63 @@ export class MovementCreateComponent implements OnInit {
     // }
   }
 
+  removeMovements(movementData: MovementData, fieldValues: FieldValues) {
+    const movementsToDelete = fieldValues.movements_todelete;
+
+    if (movementsToDelete.length > 0) {
+      movementsToDelete.forEach(movementToDelete => {
+        // tslint:disable-next-line:max-line-length
+        const foundIndex = movementData.field.domain_values.findIndex(extDomain => extDomain.value === movementToDelete);
+        if (foundIndex !== -1) {
+          movementData.field.domain_values.splice(foundIndex, 1);
+          fieldValues.numberof_deletedmovements++;
+        }
+      });
+    }
+  }
+
+  processMovementRelationOperation(foundModuleForm: ModuleFormData, movementData: MovementData) {
+    const processMovementRel: Array<ProcessMovementRel> = foundModuleForm.processMovementRel;
+    if (processMovementRel !== undefined) {
+      processMovementRel.forEach(rel => {
+        const relation: ProcessMovementRel = rel[0];
+        const islemTuru = relation.islem_turu;
+        if (this.moduleData['islem_turu'] === islemTuru) {
+          const fieldValuesArray: Array<Array<FieldValues>> = relation.field_values;
+
+          const fieldValues = fieldValuesArray[0][0];
+          const fieldValue = fieldValues.field_value[0];
+          const notData = fieldValue[0].not_values;
+          const isData = fieldValue[0].values;
+
+          if (this.moduleData[fieldValue[0].name] !== null && this.moduleData[fieldValue[0].name] !== null) {
+            if (notData.length > 0) {
+              if (fieldValue.length === 1) {
+                if (!notData.includes(this.moduleData[fieldValue[0].name])) {
+                  this.removeMovements(movementData, fieldValues);
+                }
+              } else if (fieldValue.length === 2) {
+                if (!notData.includes(this.moduleData[fieldValue[0].name]) && !notData.includes(this.moduleData[fieldValue[1].name])) {
+                  this.removeMovements(movementData, fieldValues);
+                }
+              }
+            } else if (isData.length > 0) {
+              if (fieldValue.length === 1) {
+                if (isData.includes(this.moduleData[fieldValue[0].name])) {
+                  this.removeMovements(movementData, fieldValues);
+                }
+              } else if (fieldValue.length === 2) {
+                if (isData.includes(this.moduleData[fieldValue[0].name]) && isData.includes(this.moduleData[fieldValue[1].name])) {
+                  this.removeMovements(movementData, fieldValues);
+                }
+              }
+            }
+          }
+        }
+      });
+    }
+  }
+
   customizeDomainValues(criterionName: string, criteriontype: number) {
     if (this.moduleData[criterionName] === criteriontype) {
       // Kamulaştırma'da (type:8) talep türüne göre hareketler değişmeli
@@ -1141,4 +1222,13 @@ export class MovementCreateComponent implements OnInit {
 
