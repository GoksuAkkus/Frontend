@@ -0,0 +1,42 @@
+import { Component, OnInit, Input, Output, EventEmitter } from '@angular/core';
+import { EsriMapComponent } from '../esri-map/esri-map.component';
+import esri = __esri;
+import { DialogRef } from '../dialog/dialog-ref';
+import { DialogService } from '../dialog/dialog.service';
+import { InfoComponent } from '../info/info.component';
+@Component({
+  selector: 'app-deneme',
+  templateUrl: './deneme.component.html',
+  styleUrls: ['./deneme.component.scss']
+})
+export class DenemeComponent implements OnInit {
+  _mapView: esri.MapView;
+  @Output() featureSet = new EventEmitter<esri.FeatureSet>();
+  @Input()
+  set mapView(mapView) {
+    this._mapView = mapView;
+  }
+
+  get mapView() {
+    return this._mapView;
+  }
+
+  constructor(private dialogService: DialogService) { }
+
+  ngOnInit() {
+
+  }
+  onClickDeneme() {
+    const selectedLayer = <esri.FeatureLayer>this._mapView.map.layers.find(layer => layer.title === 'Yapılar');
+    const queryProperties: esri.QueryProperties = {
+      where: `objectid= 6328`,
+      outFields: ['*'],
+      returnGeometry: true
+    };
+
+    selectedLayer.queryFeatures(queryProperties).then(result => {
+      this.featureSet.emit(result);
+    });
+  }
+}
+
