+  dotToCommaMethod(feature: esri.Graphic) {
+    const attributes = feature.attributes;
+    const keyValueArray: Array<KeyValue> = [];
+    const keyArray = Object.keys(attributes);
+    const valueArray = Object.values(attributes);
+
+    for (let i = 0; i < keyArray.length; i++) {
+      const keyValueElement = new KeyValue();
+      keyValueElement.key = keyArray[i];
+      keyValueElement.value = valueArray[i];
+      keyValueArray.push(keyValueElement);
+    }
+
+    keyValueArray.forEach(keyVal => {
+      if (String(keyVal.value).includes('.')) {
+        keyVal.value = String(keyVal.value).replace('.', ',');
+        feature.attributes[keyVal.key] = keyVal.value;
+      }
+    });
+    return feature;
+  }
+
