     });
   }
 
+  createPolygonPoints(lat: number, lon: number) {
+    const firstPoint = this.createGeodesicCoordinatePoint(lat, lon, this.en, 1.5707963268);
+    const secondPoint = this.createGeodesicCoordinatePoint(firstPoint.lat, firstPoint.lon, this.boy, 3.1415926536);
+    const thirdPoint = this.createGeodesicCoordinatePoint(secondPoint.lat, secondPoint.lon, this.en, 4.7123889804);
+
+    const polygonPoints = [[lon, lat], [firstPoint.lon, firstPoint.lat], [secondPoint.lon, secondPoint.lat],
+    [thirdPoint.lon, thirdPoint.lat]];
+
+    return polygonPoints;
+  }
+
+  createGeodesicCoordinatePoint(firstLat, firstLon, distance, brng) {
+    firstLat = firstLat * (Math.PI / 180);
+    firstLon = firstLon * (Math.PI / 180);
+    const distanceRate = distance / this.earthRadius;
+    const secondLat = Math.asin(Math.sin(firstLat) * Math.cos(distanceRate) +
+      Math.cos(firstLat) * Math.sin(distanceRate) * Math.cos(brng));
+    const secondLon = firstLon + Math.atan2(Math.sin(brng) * Math.sin(distanceRate) * Math.cos(firstLat),
+      Math.cos(distanceRate) - Math.sin(firstLat) * Math.sin(secondLat));
+    const secondLatLonObj = { lat: secondLat * 180 / Math.PI, lon: secondLon * 180 / Math.PI };
+    return secondLatLonObj;
+  }
+
   findArcTan(currentPrev: number, currentCenter: number) {
     const angle = Math.atan2(currentCenter, currentPrev);
     return angle;
