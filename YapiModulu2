diff --git a/src/app/yapi-bilgileri-info/yapi-bilgileri-genericinfo/yapi-bilgileri-genericinfo.component.ts b/src/app/yapi-bilgileri-info/yapi-bilgileri-genericinfo/yapi-bilgileri-genericinfo.component.ts
new file mode 100644
index 0000000000000000000000000000000000000000..da3ea234dec7e9a605f5703e6949a03a0cc1d8e0
--- /dev/null
+++ b/src/app/yapi-bilgileri-info/yapi-bilgileri-genericinfo/yapi-bilgileri-genericinfo.component.ts
@@ -0,0 +1,79 @@
+import { Component, OnInit, Input } from '@angular/core';
+import { DialogConfig } from '../../dialog/dialog-config';
+import { InfoData, FieldInfo } from '../../yapi-info';
+
+@Component({
+  selector: 'app-yapi-bilgileri-genericinfo',
+  templateUrl: './yapi-bilgileri-genericinfo.component.html',
+  styleUrls: ['./yapi-bilgileri-genericinfo.component.scss']
+})
+export class YapiBilgileriGenericinfoComponent implements OnInit {
+
+  _tabData: InfoData;
+  @Input()
+  set tabData(tabData: InfoData) {
+    this._tabData = tabData;
+    if (this._tabData !== undefined) {
+      if (this._tabData.tabName === 'Yapısal Bilgi') {
+
+        const katYuksekligiIndexes = [];
+        const kotYuksekligiIndexes = [];
+
+        const katYuksekligiFields = this._tabData.fields.filter(field => field.fieldName.includes('kat_yuksekligi_'));
+        const kotYuksekligiFields = this._tabData.fields.filter(field => field.fieldName.includes('kot_yuksekligi_'));
+
+        if (katYuksekligiFields.length !== 0) {
+
+          for (let i = 0; i < katYuksekligiFields.length; i++) {
+
+            const index = this._tabData.fields.indexOf(katYuksekligiFields[i]);
+            katYuksekligiIndexes.push(index);
+
+          }
+          this.tabData.fields.splice(katYuksekligiIndexes[0], (katYuksekligiIndexes.length));
+        }
+        if (kotYuksekligiFields.length !== 0) {
+
+          for (let i = 0; i < kotYuksekligiFields.length; i++) {
+
+            const index = this._tabData.fields.indexOf(kotYuksekligiFields[i]);
+            kotYuksekligiIndexes.push(index);
+
+          }
+          this.tabData.fields.splice(kotYuksekligiIndexes[0], (kotYuksekligiIndexes.length));
+        }
+      }
+    }
+  }
+  get tabData() {
+    return this._tabData;
+  }
+  constructor() { }
+
+  ngOnInit() {
+  }
+  getDateString(fieldName: string) {
+    return new Date(this.tabData.properties[fieldName]).toLocaleDateString();
+  }
+
+  getDomainString(field: FieldInfo) {
+    const fieldName = field.fieldName;
+    const tabData = this.tabData;
+    const properties = tabData.properties;
+    const fieldValue = properties[fieldName];
+    const numberFieldValue = Number(fieldValue);
+    if (field.domain[0]['code_text'] !== undefined) {
+      const domains = field.domain;
+
+      const foundDomain = domains.find(domain => {
+        if (Number(domain['code_text']) === numberFieldValue) {
+          return true;
+        }
+      });
+
+      return foundDomain['value'];
+    } else {
+      return fieldValue;
+    }
+  }
+}
