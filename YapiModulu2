+  createColDefForYapi(alias: string, fieldName: string, type: 'number' | 'text' | 'domain') {
+
+    const colDef = {
+      headerName: alias,
+      colId: fieldName,
+      field: fieldName,
+    } as ColDef;
+    if (type === 'number') {
+      colDef.filter = 'agNumberColumnFilter';
+    }
+    return colDef;
+  }
+
