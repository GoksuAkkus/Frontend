+
+
+  createWhereQueryOfYapiFilter(filterObject: Object) {
+    const filter = filterObject['filter'];
+    const type = filterObject['type'];
+    let filterTo = 0;
+
+    switch (type) {
+      case 'contains': {
+        const contains = ` LIKE '%${filter}%' `;
+        return contains;
+      }
+      case 'notContains': {
+        const notContains = ` NOT LIKE '%${filter}%' `;
+        return notContains;
+      }
+      case 'endsWith': {
+        const endsWith = ` LIKE '%${filter}' `;
+        return endsWith;
+      }
+      case 'startsWith': {
+        const startsWith = ` LIKE '${filter}'% `;
+        return startsWith;
+      }
+      case 'equals': {
+        const equal = ` = ${filter} `;
+        return equal;
+      }
+      case 'notEqual': {
+        const notEqual = ` <> ${filter} `;
+        return notEqual;
+      }
+      case 'lessThan': {
+        const lessThan = ` < ${filter} `;
+        return lessThan;
+      }
+      case 'lessThanOrEqual': {
+        const lessThanOrEqual = ` <= ${filter} `;
+        return lessThanOrEqual;
+      }
+      case 'greaterThan': {
+        const greaterThan = ` > ${filter} `;
+        return greaterThan;
+      }
+      case 'greaterThanOrEqual': {
+        const greaterThanOrEqual = ` >= ${filter} `;
+        return greaterThanOrEqual;
+      }
+      case 'inRange': {
+        if (filterObject['filterTo']) {
+          filterTo = filterObject['filterTo'];
+        }
+        const inRange = ` BETWEEN ${filter} AND ${filterTo} `;
+        return inRange;
+      }
+    }
+
+  }
+
+  filterYapi(filterModelElement: string, filterModelProperties: Object, operator?) {
+    const type = filterModelProperties['type'];
+    const filter = filterModelProperties['filter'];
+
+    const filterType = filterModelProperties['filterType'];
+    let whereQuery = '';
+    let firstConditionWhereQuery = '';
+    let secondConditionWhereQuery = '';
+
+    if (filterType === 'text') {
+      filterModelProperties['filter'] = filter.toLocaleUpperCase();
+      filterModelElement = ` tr_upper(${filterModelElement}) `;
+    }
+
+    // OPERATOR VARSA
+    if (operator !== undefined) {
+
+      firstConditionWhereQuery = this.createWhereQueryOfYapiFilter(filterModelProperties['condition1']);
+      secondConditionWhereQuery = this.createWhereQueryOfYapiFilter(filterModelProperties['condition2']);
+      whereQuery = filterModelElement + ' ' + firstConditionWhereQuery + operator + ' ' + filterModelElement + secondConditionWhereQuery;
+
+    } else {
+      whereQuery =  filterModelElement + ' ' + this.createWhereQueryOfYapiFilter(filterModelProperties);
+    }
+    return whereQuery;
+
+  }
