+import { Component, OnInit, Input, Output, EventEmitter, ViewChild, ElementRef, OnDestroy } from '@angular/core';
+import { InfoData, ModelData } from '../yapi-info';
+import { HttpClient } from '@angular/common/http';
+import { environment } from 'src/environments/environment';
+import { DialogService } from '../dialog/dialog.service';
+import { AcceptDialogComponent } from '../accept-dialog/accept-dialog.component';
+import { DialogRef } from '../dialog/dialog-ref';
+import { MessageDialogComponent } from '../message-dialog/message-dialog.component';
+
+@Component({
+  selector: 'app-yapi-bilgileri-info',
+  templateUrl: './yapi-bilgileri-info.component.html',
+  styleUrls: ['./yapi-bilgileri-info.component.scss']
+})
+
+
+
+export class YapiBilgileriInfoComponent implements OnInit {
+  @ViewChild('tabBar', { static: true }) private tabBarEl: ElementRef;
+
+  @Output() infoClose = new EventEmitter();
+  @Output() tabNames = new EventEmitter();
+  @Output() yapiInfoBack = new EventEmitter();
+
+  infoTitle = 'YAPI BİLGİLERİ';
+  tabs: Array<string> = [];
+  _infoData: ModelData;
+
+  _tabData: InfoData;
+  _tableName: string;
+  _personel_KurumKodu: number;
+  guncellikDurumDialogRef: DialogRef;
+  @Input()
+  set personel_KurumKodu(personel_KurumKodu: number) {
+    this._personel_KurumKodu = personel_KurumKodu;
+  }
+  get personel_KurumKodu() {
+    return this._personel_KurumKodu;
+  }
+  @Input()
+  set infoData(infoData: ModelData) {
+    this._infoData = infoData;
+    console.log(infoData);
+
+    if (this._infoData !== undefined) {
+        this.tabs = ['Bina', 'Yapısal Bilgi', 'Deprem', 'Enerji'];
+      }
+
+      this._tabData = this._infoData.mainInformation;
+      this._tabData.tabName = 'Bina';
+
+      setTimeout(this.defaultTabInit.bind(this));
+
+  }
+  get infoData() {
+    return this._infoData;
+  }
+
+  constructor(private http: HttpClient, private dialogService: DialogService) { }
+
+  ngOnInit() {
+
+  }
+
+  defaultTabInit() {
+    if (this.tabBarEl !== undefined) {
+      const tabBar: HTMLElement = this.tabBarEl.nativeElement;
+      const defaultTab: HTMLElement = tabBar.firstElementChild as HTMLElement;
+      if (defaultTab.classList.contains('tab')) {
+        const activeTabs = tabBar.querySelectorAll('.active');
+        if (activeTabs.length > 0) {
+
+          const activeTabArray = Array.from(activeTabs);
+          activeTabArray.forEach(tab => {
+            if (tab.previousElementSibling !== null) {
+              tab.classList.toggle('active');
+            }
+          });
+        }
+
+        if (!defaultTab.classList.contains('active')) {
+          defaultTab.classList.toggle('active');
+        }
+
+        const tabInk: HTMLElement = tabBar.lastElementChild as HTMLElement;
+        tabInk.style.left = '0';
+        tabInk.style.width = defaultTab.offsetWidth + 'px';
+      }
+    }
+  }
+
+  closeInfo() {
+    this.infoClose.emit();
+  }
+
+  tabChange(event: Event) {
+
+    if (this.guncellikDurumDialogRef !== undefined) {
+      return;
+    }
+    const targetElement: HTMLElement = event.target as HTMLElement;
+    if (!targetElement.classList.contains('active')) {
+      const tabBar: HTMLElement = this.tabBarEl.nativeElement;
+      const activeElemenmt = tabBar.querySelector('.active');
+
+      if (activeElemenmt !== null) {
+        activeElemenmt.classList.toggle('active');
+      }
+
+      targetElement.classList.toggle('active');
+
+      switch (targetElement.innerHTML) {
+        case 'Bina':
+          this._tabData = this._infoData.mainInformation;
+          this._tableName = undefined;
+          break;
+        case 'Yapısal Bilgi':
+          this._tabData = this._infoData.extraInformation.yapisal_bilgi;
+          this._tableName = 'yapisal_bilgi';
+          this._tabData.tabName = 'Yapısal Bilgi';
+          break;
+        case 'Deprem':
+          this._tabData = this._infoData.extraInformation.yapisal_durum;
+          this._tableName = 'yapisal_durum';
+          this._tabData.tabName = 'Deprem';
+          break;
+        case 'Enerji':
+          this._tabData = this._infoData.extraInformation.yapi_enerji;
+          this._tableName = 'yapi_enerji';
+          this._tabData.tabName = 'Enerji';
+      }
+    }
+  }
+  onClickBackButton() {
+    this.yapiInfoBack.emit();
+  }
+}
+
