@@ -0,0 +1,24 @@
+export interface ModelData {
+    extraInformation: ExtraInfoData;
+    mainInformation: InfoData;
+}
+
+export interface ExtraInfoData {
+    yapisal_bilgi: InfoData;
+    yapisal_durum: InfoData;
+    yapi_enerji: InfoData;
+}
+
+export interface InfoData {
+    // tslint:disable-next-line:ban-types
+    properties: Object;
+    fields: Array<FieldInfo>;
+    tabName: string;
+}
+
+export interface FieldInfo {
+    alias: string;
+    fieldName: string;
+    type: 'number' | 'text' | 'date' | 'domain';
+    domain: Array<string>;
+}
