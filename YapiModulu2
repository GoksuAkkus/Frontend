+  createColumnDefinitionsForYapi(layerName: string) {
+    const columnDefs: Array<ColDef> = [];
+    if (layerName === 'yapi_enerji') {
+      const temel_isinma_kaynagi = this.createColDefForYapi('Temel Isınma Kaynağı', 'temel_isinma_kaynagi', 'domain');
+      const iklimlendirme_ekipmanlari = this.createColDefForYapi('İklimlendirme Ekipmanları', 'iklimlendirme_ekipmanlari', 'domain');
+      const bina_isi_yalitimi = this.createColDefForYapi('Bina Isı Yalıtımı', 'bina_isi_yalitimi', 'domain');
+      const cati_isi_yalitimi = this.createColDefForYapi('Çatı Isı Yalıtımı', 'cati_isi_yalitimi', 'domain');
+      const pencere_dogramalari = this.createColDefForYapi('Pencere Doğramaları', 'pencere_dogramalari', 'domain');
+      const dogalgaz_donusum_durumu = this.createColDefForYapi('Doğalgaz Dönüşüm Durumu', 'dogalgaz_donusum_durumu', 'domain');
+      const elektrik_aboneno = this.createColDefForYapi('Elektrik Abone No', 'elektrik_aboneno', 'number');
+      const su_aboneno = this.createColDefForYapi('Su Abone No', 'su_aboneno', 'number');
+      const dogalgaz_aboneno = this.createColDefForYapi('Doğalgaz Abone No', 'dogalgaz_aboneno', 'number');
+      const tesisat_no = this.createColDefForYapi('Tesisat No', 'tesisat_no', 'number');
+      const bina_sira_no = this.createColDefForYapi('Bina Sıra No', 'bina_sira_no', 'number');
+
+      columnDefs.push(bina_sira_no, temel_isinma_kaynagi,
+        iklimlendirme_ekipmanlari,
